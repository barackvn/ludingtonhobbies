<odoo>
    <data>
        <record id="pos_session_form_inherited" model="ir.ui.view">
            <field name="name">pos.session.form.inherited</field>
            <field name="model">pos.session</field>
            <field name="type">form</field>
            <field name="inherit_id" ref="point_of_sale.view_pos_session_form"/>
            <field name="arch" type="xml">
                <xpath expr="//header" position="inside">
                    <button id="action_pos_box_out_id" name="%(point_of_sale.action_pos_box_out)d" type="action" string="Cash In/Out" class="oe_highlight"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>